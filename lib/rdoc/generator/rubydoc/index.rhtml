    <!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
      <!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
        <head>
     <!-- RD version: <%= RDoc::Generator::Rubydoc::VERSION  %>  -->
          <meta charset="utf-8">

          <!-- Use the .htaccess and remove these lines to avoid edge case issues.
          More info: h5bp.com/b/378 -->
          <!-- meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" -->

          <title>Index of <%= @simple_files.empty? ? '' : 'Files, ' %> Classes &amp; Methods in <%= h @options.title %><%= @ruby_version ? " (Ruby #{@ruby_version}) " : '' %></title>

          <%# link rel="stylesheet" href="/css/inuit.css" %>
          <%# link rel="stylesheet" href="/css/rdoc.css" %>
          <%# link rel="stylesheet" href="/css/grid.inuit.css" %>
          <%# link rel="stylesheet" href="/css/2011.css" %>


          <!-- link rel="stylesheet" href="/css/rdoc.css" >
          <link rel="stylesheet" href="/css/grid.inuit.css" -->
  <!-- raw_prefix  = '<%= raw_prefix %>'   -->
   <link rel="stylesheet" href="<%= rel_prefix %>css/obf.css" >
        
        </head>
        <body class="index">
<div id='actionbar' >
<div class='wrapper'>
 <%= @menu_bar %>
</div>
</div> <!-- end action bar -->
          <div class='wrapper'>
          <% $stderr.sync = true %>
          <header>
          <div id="header">
            <h1><%=  @options.title  %></h1>
          </div>
          <% if File.exist? 'header_text.html' %>
          <%= IO.read 'header_text.html'%> 
          <% end %>
          </header>
          <div id='wrapper'>
            <%= @ruby_version ? "<h4>Ruby #{@ruby_version}</h4>" : '' %>
            <% unless @files_to_display.empty? %>
            <div id="file-index">
              <div class="title">Files</div>
              <div class="entries">
                <% @files_to_display.each do |file| %>
                <p><a href="<%= file.path %>"><%= h file.absolute_name %></a></p>
                <% end %>
              </div>
            </div>
            <% end %>
            <div id="class-index">
              <div class="title">
                <span class="text">Classes</span>
                <input type="text" value="" class="search-field" >
              </div>
              <div class="entries"><% @all_classes_and_modules.each do |k| %>
                <p class="<%= k.any_content ? k.type : k.type + ' nodoc' %>"><span class="type ng-cm-type"><%= k.type[0..0].upcase %></span><a href="<%= k.path %>"><%= k.full_name + (k.is_alias_for ? ' &rarr; ' << k.is_alias_for.full_name : '') %></a></p>
                <% end %>
              </div>
            </div>

            <div id="method-index">
              <div class="title">
                <span class="text">Methods</span>
                <input type="text" value="" class="search-field" />

              </div>
              <div class="entries">
                <% @all_methods.each do |m| %><% ptag = m.documented? ? '<p>' : '<p class="nodoc">' %><%= ptag %><span class="ng-cm-type type"><%= m.name_prefix %></span><a href="<%= m.path %>"><%= h(m.name) %> (<%= m.parent.full_name %>)</a></p>
                  <% end %>
                </div>
              </div>
            <div id='dynamic'></div>
            </div>
            <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
            <script>window.jQuery || document.write('<script src="js/libs/jquery-1.6.2.min.js"><\/script>')</script>

              <script src="js/jquery.quicksearch.js" type="text/javascript" charset="utf-8"></script>
              <script src="js/indexFrame.js" type="text/javascript" charset="utf-8"></script>
              <script src="js/libs/modernizr-2.0.6.min.js"></script>


              <!-- scripts concatenated and minified via ant build script-->
              <script defer src="js/plugins.js"></script>
              <script defer src="js/script.js"></script>
              <!-- end scripts-->

              <!-- Prompt IE 6 users to install Chrome Frame. Remove this if you want to support IE 6.
              chromium.org/developers/how-tos/chrome-frame-getting-started -->
              <!--[if lt IE 7 ]>
              <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.3/CFInstall.min.js"></script>
              <script>window.attachEvent('onload',function(){CFInstall.check({mode:'overlay'})})</script>
              <![endif]-->

              </div>

              <div id="footer">
                <% if @footer_blurbs && !@footer_blurbs.empty? %>
                <%= @footer_blurbs[rand(100) % @footer_blurbs.size] %>
                <% end%>
                <p><small>Generated with the <a href="http://neurogami.com/code/">Rubydoc Rdoc Generator</a> <%= RDoc::Generator::Rubydoc::VERSION %></small>.</p>
              </div>
             <%= render_if_exists "anayltics.html" %>

            </body>
          </html>

